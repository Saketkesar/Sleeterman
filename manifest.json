{
    "manifest_version": 2,
    "name": " Sleeterman protector",
    "version": "1.0",
    "description": " Sleeterman created by Saket kesar",
    "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
    "background": {
        "scripts": ["background.js"]
        
    },

    "browser_action": {
        "default_title": "Sleeterman",
        "default_popup": "popup.html"
    },
    
    "icons": {
        "16": "icons/logo_128x128.png",
        "48": "icons/logo_128x128.png",
        "128": "icons/logo_128x128.png"
    }
    
}